                </ul>
            
                <button type="button" class="btn  btn-outline-danger text-black my_orders"  onclick="window.location.href='/listorders'" >My Orders</button>
              </div> 
              <!-- .card-body -->
            </div>





            <div class="card mb-4 shadow">
              <div class="card-body my-4">
                <h3 class="h5 mb-3">Address</h3>
                <% if(useraddress && useraddress.length > 0) { %>
                <div class="address">
                  <% for (let i = 0 ; i<Math.min(useraddress.length, 1) ; i++) { %>
                      <div class="form-check position-relative">
                        <input class="form-check-input" type="radio" name="address" value="<%= useraddress[i]._id %>" id="<%= useraddress[i]._id %>" checked>
                        <label class="form-check-label" for="<%= useraddress[i]._id %>">
                          <h6 class="mb-1"><%= useraddress[i].firstname %> <%= useraddress[i].lastname %></h6>
                          <p class="text-muted mb-0"><%= useraddress[i].address %>, <%= useraddress[i].city %>, <%= useraddress[i].state %>-<%= useraddress[i].zipcode %>, <%= useraddress[i].country %></p>
                          <p class="text-muted mb-0">Mobile: <%= useraddress[i].phone %></p>
                        </label>
                        <a href="/deleteaddress?id=<%= useraddress[i]._id %>" class="border-0 bg-transparent position-absolute end-0 translate-middle-y" ><span class="material-symbols-outlined" data-id="<%= useraddress[i]._id %>">delete</span></a> 
                      </div>
                      
                    
                    <hr>
                   
                  <% } %>
                </div>
                <% if (useraddress.length > 1) { %>
                <div class="address-more" style="display:none">
                  <% for (let i = 1 ; i<useraddress.length ; i++) { %>
                  <div class="form-check">
                    <input class="form-check-input" type="radio" name="address" value="<%= useraddress[i]._id %>" id="<%= useraddress[i]._id %>">
                    <label class="form-check-label" for="<%= useraddress[i]._id %>">
                      <h6 class="mb-1"><%= useraddress[i].firstname %> <%= useraddress[i].lastname %></h6>
                      <p class="text-muted mb-0"><%= useraddress[i].address %>, <%= useraddress[i].city %>, <%= useraddress[i].state %>-<%= useraddress[i].zipcode %>, <%= useraddress[i].country %></p>
                      <p class="text-muted mb-0">Mobile: <%= useraddress[i].phone %></p>
                    </label>
                    <button class="border-0 bg-transparent position-absolute end-0 translate-middle-y" onclick="window.Location.href ='/deleteaddress?id=<%= useraddress[i]._id %>'"><span class="material-symbols-outlined" data-id="<%= useraddress[i]._id %>">delete</span></button> 

                  </div>
                  <hr>
                  <% } %>
                </div>
                <button class="btn btn-link show-more-btn" type="button">Show More</button>
                <% } %>
                <% } else { %>
                <p class="text-muted">No address added</p>
                <% } %>
                <br><br><br><br>
                <button type="button" class="btn btn-outline-danger text-black my_orders justify-content-between" data-target="my_orders" onclick="window.location.href='/addnewaddress'">Add Address</button>
              </div>
            </div>
            
            <script>
              const addressMore = document.querySelector('.address-more');
              const showMoreBtn = document.querySelector('.show-more-btn');
            
              if (addressMore && showMoreBtn) {
                showMoreBtn.addEventListener('click', function () {
                  addressMore.style.display = addressMore.style.display === 'none' ? 'block' : 'none';
                  showMoreBtn.innerText = addressMore.style.display === 'none' ? 'Show More' : 'Show Less';
                });
              }
            </script>
            
                
                  
                 
            
            
            
          
